[diff]
   tool = nvimdiff
[difftool]
   prompt = false
[merge]
   tool = nvimdiff
[credential]
   helper = store --file ~/.git/credentials
[filter "lfs"]
   required = true
   clean = git-lfs clean -- %f
   smudge = git-lfs smudge -- %f
   process = git-lfs filter-process
[diff "zip"]
   textconv = unzip -c -a
